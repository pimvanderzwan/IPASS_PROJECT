#include "hwlib.hpp"

int main()
{
    namespace target = hwlib::target;
   
   auto scl = target::pin_oc( target::pins::d6 );
   auto sda = target::pin_oc( target::pins::d7 );
   
   auto i2c_bus = hwlib::i2c_bus_bit_banged_scl_sda( scl,sda );
   
   auto display = hwlib::glcd_oled_i2c_128x128_direct( i2c_bus, 0x3C );  
    
    display.clear();

        uint8_t img3[8192];
        for (size_t i = 0; i < 8192; i++){img3[i]= 0;}
            int x = 0;
            int y = 0;

char string [17384] ="";
        
for (size_t i = 0; i < 16384; i=i+2)
        { 
          img3[i/2]=(string[i+1]*16 + string[i] -'0');
        }
for (size_t i =0; i < 8192; i++ ){
    if (x > 63){y++; x = 0;}
    if (img3[i] ==0) {hwlib::cout << "location: " << x << ","<< y << " " << img3[i] << "\n";}
    display.pixels_byte_write(hwlib::xy(x,y), img3[i]);
    x++;
    }
}
